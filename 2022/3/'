#include<iostream>
#include<fstream>
#include<string>
#include<map>
#include<algorithm>
using namespace std;
int main()
{
  ifstream file("input.txt");
  string line;
  while (getline(file, line)) {
    string first = line.substr(0,line.length()/2),
           second = line.substr(line.length()/2, line.length());
    map<char, int>freqFirst;
    map<char, int>freqSecond;
    for (auto c : first ) {
      freqFirst[c]++;
    }
    for (auto c : second) {
      freqSecond[c]++;
    }
    //cout << first << " " << second << endl;
    
    cout << endl << endl;
  }
  return 0;
}
